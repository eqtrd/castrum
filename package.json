{
  "name": "kirby-starterkit-eqtrd",
  "version": "1.0.0",
  "description": "A fresh kirby starterkit working with Composer/Npm and webpack",
  "main": "index.js",
  "scripts": {
    "browsersync": "",
    "dev": "APP_ENV=dev node --no-warnings .config/browsersync.js & vite --config .config/vite.config.js",
    "server": "APP_ENV=dev php -S localhost:3000 -t www www/kirby/router.php",
    "build": "vite build --config .config/vite.config.js",
    "fonts": "node .config/fonts.config.js",
    "preview": "npm run build && npm run server"
  },
  "type": "module",
  "author": "eqtrd",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "browser-sync": "^3.0.3",
    "fantasticon": "^3.0.0",
    "fontconvert-tool": "^1.1.2",
    "path": "^0.12.7",
    "sass": "^1.83.4",
    "vite": "^6.0.11",
    "vite-plugin-live-reload": "^3.0.4"
  },
  "browserslist": [
    "edge >= 16",
    "firefox >= 52",
    "chrome >= 57",
    "safari >= 10.1",
    "opera >= 44",
    "ios >= 10.3",
    "android >= 67",
    "and_chr >= 74",
    "and_uc >= 11.8",
    "op_mob >= 46",
    "and_ff >= 67",
    "samsung >= 6.2"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "imagesloaded": "^5.0.0",
    "jquery": "^3.7.1",
    "lazysizes": "^5.3.2"
  }
}
