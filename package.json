{
  "name": "kirby-starterkit-eqtrd",
  "version": "1.0.0",
  "description": "A fresh kirby starterkit working with Composer/Npm and webpack",
  "main": "index.js",
  "scripts": {
    "browsersync": "",
    "dev": "vite --mode development & wait-on tcp:localhost:5173 && node --no-warnings browsersync.js",
    "server": "APP_ENV=dev php -S localhost:3000 -t www www/kirby/router.php",
    "build": "vite build",
    "fonts": "node .config/fonts.config.js",
    "preview": "npm run build && npm run server"
  },
  "type": "module",
  "author": "eqtrd",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "browser-sync": "^3.0.4",
    "fantasticon": "^3.0.0",
    "fontconvert-tool": "^1.1.2",
    "path": "^0.12.7",
    "sass": "^1.87.0",
    "vite": "^6.3.2",
    "vite-plugin-live-reload": "^3.0.4",
    "wait-on": "^8.0.3"
  },
  "browserslist": [
    "edge >= 16",
    "firefox >= 52",
    "chrome >= 57",
    "safari >= 10.1",
    "opera >= 44",
    "ios >= 10.3",
    "android >= 67",
    "and_chr >= 74",
    "and_uc >= 11.8",
    "op_mob >= 46",
    "and_ff >= 67",
    "samsung >= 6.2"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "@alpinejs/collapse": "^3.14.9",
    "alpinejs": "^3.14.9",
    "flickity": "^3.0.0",
    "flickity-fade": "^2.0.0",
    "gsap": "^3.13.0",
    "imagesloaded": "^5.0.0",
    "jquery": "^3.7.1",
    "lazysizes": "^5.3.2"
  }
}
